public class PrintArr{
	public static void main (String [] args){
		int [] arr = { 1, 2, 3};
        printArray(arr);
    }

		public static void printArray(int[] a) {
			
             for (int x : a) {                      //for (int i = 0;i,arr,length; i++ ){ 
				                   //System.out.println(arr[i])  }
            System.out.println(x);                 
           }
        }
}
